{{!< default}}

<!-- LANDING -->
<div class="bg-background-block"></div>
<div class="bg-landing {{#if @blog.cover_image}}" style="background-image: url({{@blog.cover_image}}){{else}}no-cover{{/if}}">
</div>

<!-- LANDING > POPUP ORDER -->
<div class="landing-popup-order-wrapper">
  <div class="landing-popup-order-background">
  </div>
  <div class="landing-popup-order-close_btn">
    <svg x="0px" y="0px" width="40px" height="40px" viewBox="0 0 60 60">
      <line fill="none" stroke="#EDEDED" stroke-width="3" x1="58.307" y1="1.693" x2="1.693" y2="58.308"/>
      <line fill="none" stroke="#EDEDED" stroke-width="3" x1="58.307" y1="58.307" x2="1.692" y2="1.693"/>
    </svg>
  </div>

  <div class="landing-popup-order-container">
    {{#get "posts" limit="all" include="author,tags"}}
      {{#foreach posts}}
        {{#has tag="#order"}}
          <div class="landing-popup-order-title {{post_class}}">
            {{title}}
          </div>
          <div class="landing-popup-order-line {{post_class}}"></div>
          <div class="landing-popup-order-text {{post_class}}">
            {{content}}
          </div>
        {{/has}}
      {{/foreach}}
    {{/get}}
  </div>
</div>


<!-- MENU > SIDEBAR -->
<div class="menu-partial">
  {{> menu}}
</div>


<!-- LANDING > CONTAINER -->
<div class="container-fluid viewport-landing margin-container">

  <!-- LANDING > LOGO + NAVBAR -->
  <div class="margintop-landing-logo landing-logo">
    <a href="{{@blog.url}}"><img src="{{@blog.logo}}" alt="{{@blog.title}}" width="170px"/></a>
  </div>


  <!-- LANDING > WCAG TEXT SIZE -->
  <div class="landing-wcag-textsize navbar-link-style">
    <a id="wcag-link"
      href="#"
      alt="Endre tekststørrelse"
      title="For å endre tekststørrelse hold ned ctrl + for å øke og ctrl - for å minske. Ved mac bruker du cmd + og cmd -"
      data-toggle="popover"
      data-placement="bottom"
      data-contentoff="Content">
      <i class="fa fa-font"></i>
      <i class="fa fa-font fa-lg"></i>
        <span style="margin-left:6px;">Tekststørrelse</span>
    </a>
  </div>


  <!-- LANDING > NAVBAR WRAPPER -->
  <div class="margintop-landing-navbar landing-navbar-wrapper">
      <!-- LANDING > NAVBAR -->
      <div class="landing-navbar">
        {{navigation}}
      </div>
      <div class="menu-icon">
        <p style="display:inline-block; vertical-align:middle; padding-right:18px;">Meny</p>
        <i class="fa fa-bars bars-icon" style="font-size:24px;"></i>
      </div>
  </div>





  <!-- LANDING > INTRO TITLE -->
  <!-- <div class="row margintop-landing-intro"> -->
    <div class="col-sm-12 col-md-10 col-md-offset-1 resetpadding lit">
      <div class="inner margin lit">
        <div class="landing-intro-title">
          {{#get "posts" limit="all" include="author,tags"}}
            {{#foreach posts}}
              {{#has tag="#landing-intro-title"}}
                <div class="text {{post_class}}">
                  {{content}}
                </div>
              {{/has}}
            {{/foreach}}
          {{/get}}

          <div class="vikarstilling">
            {{#get "posts" limit="all" include="author,tags"}}
              {{#foreach posts}}
                {{#has tag="#vikarstilling"}}
                    {{content}}
                {{/has}}
              {{/foreach}}
            {{/get}}
          </div>
        </div>


      </div>
    </div>
  <!-- </div> -->



  <!-- LANDING > BOTTOM ELEMENTS -->
  <div class="row">
    <!-- LANDING > GOOGLE MAP -->
    <div class="landing-bottom-left">
      <div class="inner">
        <div class="landing-googlemap-circle-container">
          <a href="https://www.google.no/maps/place/Ullernklinikken+Manuellterapi+og+Rehabilitering+Sa/@59.9268115,10.6400237,17z/data=!4m13!1m7!3m6!1s0x46416d040a573a51:0xbd1edc8665da4c17!2sUllern+all%C3%A9+28,+0381+Oslo!3b1!8m2!3d59.9268088!4d10.6422124!3m4!1s0x0:0xebebf7d123257708!8m2!3d59.9270243!4d10.6426352?hl=en" alt="" target="_blank">
            <div class="landing-googlemap-box" id="map-landing">
            </div>
          </a>
        </div>
      </div>
    </div>


    <!-- LANDING > ARROW DOWN -->
    <div class="landing-bottom-center">
      <span style="width:46px;">
        <a href="#intro">
          {{#get "posts" limit="all" include="author,tags"}}
            {{#foreach posts}}
              {{#has tag="#landing-bottom-center-arrow_down"}}
                  {{content}}
              {{/has}}
            {{/foreach}}
          {{/get}}
        </a>
      </span>
    </div>

    <!-- LANDING > ORDER BUTTON -->
    <div class="landing-bottom-right">
      bestill time
    </div>

  </div>
</div>








