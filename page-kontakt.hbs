{{!< default}}

{{!-- ================================================= --}}

{{!-- PARTIAL > NAV > SECONDARY --}}
{{> navigation-secondary}}

{{!-- ================================================= --}}

<div class="menu-partial">
  {{> menu}}
</div>

{{!-- CONTENT > KONTAKT PAGE --}}

{{!-- google map right side --}}
<div class="kontakt-map" id="map-contact">
</div>

{{!-- main container  --}}
<div class="container-fluid margin-container-kontakt">
<div class="row">
  <div class="col-xs-12 col-sm-12 col-md-8 resetpadding">
    <div class="inner margin">

      <!-- ROW 1 -->
      <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-8 resetpadding">
          <div class="inner margin">
            <div class="kontakt-wrapper margintop-kontakt">
              <div class="kontakt-title">
                Kontakt
              </div>
            </div>
          </div>
        </div>
      </div> <!-- ROW 1 END -->


      <!-- ROW 2 -->
      <div class="row">
        <!-- LEFT COLUMN 2 -->
        <div class="col-xs-12 col-sm-12 col-md-9 col-md-offset-1 resetpadding">
          <!-- INNER MARGIN 2 -->
          <div class="inner margin">
            <!-- KONTAKT WRAPPER 2 -->
            <div class="kontakt-wrapper margintop-kontakt-content">
              <div class="kontakt-text">
                {{#get "posts" limit="all" include="author,tags"}}
                  {{#foreach posts}}
                    {{#has tag="#kontakt-text"}}
                        {{content}}
                    {{/has}}
                  {{/foreach}}
                {{/get}}
              </div>

              <div class="kontakt-text-2">
                <i class="fa fa-phone-square fa-lg"></i>
                <p style="margin-left:9px;">
                  {{#get "posts" limit="all" include="author,tags"}}
                    {{#foreach posts}}
                      {{#has tag="#landing-intro-text-telephone"}}
                          {{content}}
                      {{/has}}
                    {{/foreach}}
                  {{/get}}
                </p>
              </div>

              <div class="kontakt-text-2">
                <i class="fa fa-map-marker fa-lg"></i>
                <p style="margin-left:18px;">
                  {{#get "posts" limit="all" include="author,tags"}}
                    {{#foreach posts}}
                      {{#has tag="#landing-intro-text-adress"}}
                          {{content}}
                      {{/has}}
                    {{/foreach}}
                  {{/get}}
                </p>
              </div>

              <div class="kontakt-text-2 kontakt-mail-xsmobile">
                <i class="fa fa-at fa-lg"></i>
                <p style="margin-left:12px;">
                  {{#get "posts" limit="all" include="author,tags"}}
                    {{#foreach posts}}
                      {{#has tag="#kontakt-mail"}}
                          {{content}}
                      {{/has}}
                    {{/foreach}}
                  {{/get}}
                </p>
              </div>

              <div class="kontakt-line"></div>

              <div class="kontakt-kollektiv-title">
                Kollektivtransport
              </div>

              <div class="kontakt-kollektiv">
                <i class="fa fa-subway fa-lg"></i>
                <p class="margin-left-subway-icon">
                  <img src="../assets/gfx/subway-icon-t.svg" height="33px" color="green" class="kontakt-kollektiv-icon">
                </p>
                <p class="margin-left-subway-number">
                  <div class="kontakt-kollektiv-rectangle">3</div>
                </p>
                <p class="margin-left-subway-name">
                  <div class="kontakt-kollektiv-text">Åsjordet</div>
                </p>
              </div>


              <div class="kontakt-kollektiv margin-top-bus">
                <i class="fa fa-bus fa-lg"></i>
                <p class="margin-left-bus-icon">
                  <img src="../assets/gfx/subway-icon-b.svg" height="33px" color="green" class="kontakt-kollektiv-icon">
                </p>
                <p class="margin-left-bus-number">
                  <div class="kontakt-kollektiv-rectangle">23</div>
                </p>
                <p class="margin-left-bus-name">
                  <div class="kontakt-kollektiv-text">Blokkajordet</div>
                </p>
              </div>

              <div class="kontakt-kollektiv">
                <p class="margin-left-bus-number-secondary">
                  <div class="kontakt-kollektiv-rectangle">32</div>
                </p>
                <p class="margin-left-bus-name">
                  <div class="kontakt-kollektiv-text">Bjørnsletta</div>
                </p>
              </div>

              <div class="kontakt-kollektiv margin-top-ruter">
                <i class="fa fa-hashtag fa-lg"></i>
                <p class="margin-left-ruter-text kontakt-kollektiv-text-xsmobile">
                  <a class="kontakt-kollektiv-links" href="https://ruter.no/reiseplanlegger/Mellom/Fra/Din%20posisjon/til/(30117942)Ullern%20all%C3%A9%2028%20(Oslo)/etter/#st:0,sp:0,bp:0" target="_blank" alt="" >
                    <div class="kontakt-kollektiv-text">Finn din reise via Ruter.no</div>
                  </a>
                </p>
              </div>

            </div><!-- KONTAKT WRAPPER 2 END -->
          </div><!-- INNER MARGIN 2 END-->
        </div><!-- LEFT COLUMN 2 END -->
      </div> <!-- ROW 2 END -->

    </div>
  </div>
</div>




    <!-- ========== SIDEBAR ========== -->
