    <!-- <div class="col-md-4 col-md-offset-1 resetpadding">
      <div class="inner margin-kontakt">
      </div>
    </div> RIGHT COLUMN > SIDEBAR END -->



  </div><!-- ROW END -->
</div><!-- CONTAINER END -->


